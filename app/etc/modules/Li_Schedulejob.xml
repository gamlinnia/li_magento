<?xml version="1.0"?>
<config>
    <modules>
        <Li_Schedulejob>
            <active>true</active>
            <codePool>local</codePool>
        </Li_Schedulejob>
    </modules>
    <crontab>
        <jobs>
            <schedulejob_do_schedules>
                <schedule>
                    <cron_expr>* 10 * * *</cron_expr>
                </schedule>
                <run>
                    <model>schedulejob/observer::doSchedules</model>
                </run>
            </schedulejob_do_schedules>
        </jobs>
    </crontab>
</config>